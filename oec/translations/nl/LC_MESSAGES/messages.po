#, python-format
msgid ""
"In %(year)s %(country)s imported $%(import_val)s, making it the %(import_rank)s "
"largest importer in the world. "
msgstr ""
"In %(year)s %(country)s ingevoerde $ %(import_val)s, waardoor het de "
"%(import_rank)s grootste importeur ter wereld."

#: oec/profile/models.py:226
#, python-format
